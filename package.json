{
  "name": "OpenMetaOffice-Electron",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "@ant-design/icons": "4.3.0",
    "ahooks": "^3.0.0-alpha.4",
    "antd": "^4.24.12",
    "axios": "^0.21.4",
    "cos-js-sdk-v5": "^1.3.1",
    "crypto": "^1.0.1",
    "crypto-js": "^4.2.0",
    "electron-devtools-installer": "^3.2.0",
    "electron-is-dev": "^2.0.0",
    "electron-reload": "^2.0.0-alpha.1",
    "electron-store": "^8.0.1",
    "electron-updater": "^4.6.5",
    "events": "^3.3.0",
    "express": "^4.17.3",
    "i18next": "^21.6.5",
    "i18next-browser-languagedetector": "^6.1.2",
    "livekit-client": "^0.17.0",
    "livekit-react": "^0.7.1",
    "localforage": "^1.10.0",
    "md5": "^2.3.0",
    "minimist": "^1.2.8",
    "open-im-sdk-web-wasm": "0.0.53",
    "qs": "^6.10.3",
    "rc-virtual-list": "^3.4.7",
    "react": "^17.0.2",
    "react-amap": "^1.2.8",
    "react-copy-to-clipboard": "^5.0.4",
    "react-dom": "^17.0.2",
    "react-draggable": "^4.4.4",
    "react-i18next": "^11.15.3",
    "react-infinite-scroll-component": "^6.1.0",
    "react-international-phone": "4.0.4",
    "react-redux": "^7.2.6",
    "react-router": "^6.2.1",
    "react-router-dom": "^6.2.1",
    "react-scripts": "4.0.3",
    "react-scroll": "^1.8.4",
    "react-virtualized": "^9.22.3",
    "redux": "^4.1.2",
    "redux-thunk": "^2.4.0",
    "sass": "^1.71.1",
    "throttle-debounce": "^3.0.1",
    "typescript": "^4.1.2",
    "web-vitals": "^1.0.1",
    "xgplayer": "^2.31.6"
  },
  "scripts": {
    "start:renderer": "craco start",
    "build:renderer": "craco build",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "build:main": "craco build && tsc -p electron && electron-builder --macos --win",
    "build:mainWin": "craco build && tsc -p electron && electron-builder --win",
    "test": "craco test",
    "eject": "react-scripts eject",
    "el": "electron .",
    "start:main": "run-p electron:*",
    "electron:run": "wait-on http://localhost:3001 && tsc -p electron && electron .",
    "electron:main": "wait-on http://localhost:3001 && tsc -p electron -w",
    "electron:renderer": "cross-env BROWSER=none npm run start:renderer"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "rules": {
      "no-undef": "off",
      "no-restricted-globals": "off",
      "import/no-webpack-loader-syntax": "off",
      "no-unused-expressions": "off"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-decorators": "^7.16.0",
    "@craco/craco": "^6.4.0",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/crypto-js": "^4.2.2",
    "@types/electron-devtools-installer": "^2.2.0",
    "@types/jest": "^26.0.15",
    "@types/md5": "^2.3.1",
    "@types/node": "^14.18.12",
    "@types/qs": "^6.9.7",
    "@types/react": "17.0.39",
    "@types/react-copy-to-clipboard": "^5.0.2",
    "@types/react-dom": "17.0.11",
    "@types/react-scroll": "^1.8.3",
    "@types/react-virtualized": "^9.21.21",
    "@types/throttle-debounce": "^2.1.0",
    "@types/uuid": "^9.0.8",
    "babel-plugin-import": "^1.13.3",
    "craco-less": "^1.20.0",
    "cross-env": "^7.0.3",
    "electron": "^15.3.0",
    "electron-builder": "^22.13.1",
    "less": "^3.13.1",
    "less-loader": "^7.3.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.3.11",
    "react-error-overlay": "^6.0.9",
    "redux-devtools-extension": "^2.13.9",
    "source-map-explorer": "^2.5.2",
    "terser-webpack-plugin": "^4.2.3",
    "wait-on": "^6.0.0",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpackbar": "^5.0.0-3",
    "worker-loader": "^3.0.8",
    "worker-plugin": "^5.0.1"
  },
  "resolutions": {
    "//": "See https://github.com/facebook/create-react-app/issues/11773",
    "react-error-overlay": "6.0.9"
  },
  "main": "build/electron/main.js",
  "homepage": "./",
  "author": "blooming",
  "description": "Open IM",
  "email": "blooming2477@gmail.com",
  "build": {
    "publish": [
      {
        "provider": "generic",
        "url": "http://localhost:25565"
      }
    ],
    "extends": null,
    "files": [
      "build/**/*"
    ],
    "productName": "QieQie",
    "win": {
      "icon": "./build/icons/logo256x256.ico"
    },
    "mac": {
      "target": [
        "dmg",
        "zip"
      ],
      "icon": "./build/icons/logo512x512.png",
      "entitlements": "entitlements.mac.plist",
      "hardenedRuntime": true,
      "extendInfo": {
        "NSMicrophoneUsageDescription": "请允许本程序访问您的麦克风",
        "NSCameraUsageDescription": "请允许本程序访问您的摄像头"
      }
    },
    "linux": {
      "icon": "./build/icons/logo512x512.png",
      "target": "deb",
      "maintainer": "QieQie"
    },
    "nsis": {
      "oneClick": false,
      "perMachine": true,
      "allowElevation": true,
      "allowToChangeInstallationDirectory": true,
      "installerIcon": "./build/icons/app.ico",
      "uninstallerIcon": "./build/icons/app.ico",
      "installerHeaderIcon": "./build/icons/app.ico",
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true,
      "shortcutName": "QieQie"
    },
    "asar": false,
    "extraResources": {
      "from": "./electron/exec/",
      "to": "exec"
    }
  }
}
